<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" stylesheets="@../css/AdminPanel.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.warehouse.controller.AdminPanel">
   <children>
      <VBox alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="350.0" prefWidth="550.0" styleClass="mainPanel" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <Label styleClass="welcomeLabel" text="Welcome back Administrator">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <Button fx:id="addUser" mnemonicParsing="false" onAction="#handleAddUser" styleClass="addUserButton" text="ADD USER">
               <VBox.margin>
                  <Insets bottom="10.0" top="50.0" />
               </VBox.margin></Button>
            <Button fx:id="manageUsers" mnemonicParsing="false" onAction="#handleRemoveUser" styleClass="manageUsersButton" text="REMOVE USER">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin></Button>
            <Button fx:id="manageFunds" mnemonicParsing="false" onAction="#handleManageFunds" styleClass="manageFundsButton" text="ADD FUNDS">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin></Button>
            <Button fx:id="logout" mnemonicParsing="false" onAction="#handleLogout" styleClass="logoutButton" text="LOGOUT" />
         </children>
      </VBox>
   </children>
</AnchorPane>
